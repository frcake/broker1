
<div class="indexbox">
	<% @classified.each do |f| %>

	<div class="center-div" id="index">
		
		<%= link_to classified_path(f) , :class => "link" do %>
			<div class="media">
				<div class="mediabox">
					
						<div class="media-left" href="#">
							<% f.photos.each do |p| %>

							<!-- <img class="media-object" src="..." alt="Generic placeholder image">-->
							<%= image_tag p.image , :class => "media-object" %>

							<%end%>
						</div>
						<div class="media-body">
								<h5 class="media-heading"> <%= f.title %></h5>
										<div class="price">										
											<h3><%= f.price %></h3>
										</div>
							</div>
						</div>
				</div>
			</div>

			<%end%>
	<%end%>

	</div>

</div>