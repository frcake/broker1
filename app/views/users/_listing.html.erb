<div class="new">
    <div class="container-fluid">
        <section class="container">
            <div class="container-page" id="index">
                <% @user.classifieds.each do |f| %>
                <%= link_to classified_path(f) do %>


                <ul>
                    <li>
                       <% f.photos.each do |p| %>

                       <%= image_tag p.image %>

                       <%end%>
                       <h4><p><%= f.title %></p></h4>
                       <p>
                        <%= f.make %>
                        <%= f.model %>
                        <%= f.year %>
                        <%= f.created_at.to_formatted_s(:short) %>
                    </p>
                    <p>
                        <%= f.make_country %>
                        <%= f.price %>
                        <%= link_to "Edit", editlisting_path(f) %>
                        <%= link_to 'Destroy',f, :confirm => "Are you sure?" ,:method => :delete %>
                    </p>
                    <p></p>
                </li>
            </ul>




            <%end%>
            <% end %>
        </div>
    </section>
</div>
</div>

